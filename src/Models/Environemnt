class Env {
    constructor(ACPower, ElecPump, CargoDoor) {
      this.ACPower = ACPower;
      this.ElecPump = ElecPump;
      this.CargoDoor = CargoDoor;
    }
  
    // Getters
    getACPower() {
      return this.ACPower;
    }
    getElecPump() {
      return this.ElecPump;
    }
    getCargoDoor() {
      return this.CargoDoor;
    }
  
    // Setters
    setACPower(ACPower) {
      this.ACPower = ACPower;
    }
    setElecPump(ElecPump) {
      this.ElecPump = ElecPump;
    }
    setCargoDoor(CargoDoor) {
      this.CargoDoor = CargoDoor;
    }
  }
  
  export default Env; // Ensure you're using 'export default'
  